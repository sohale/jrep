{
  "name": "jrep1",
  "version": "0.5.0",
  "description": "A new generation grep that uses Javascript",
  "main": "src/jrep.js",
  "bin": {
    "jrep": "src/jrep.js"
  },
  "scripts": {
    "test:e2e": "test/e2e-test.bash",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "#npm run lint",
    "test:unit": "NODE_ENV=test nyc ./node_modules/mocha/bin/mocha --config test/.mocharc.js",
    "test": "npm run test:unit"
  },
  "keywords": [
    "grep",
    "awk",
    "sed",
    "linux",
    "scripting",
    "bash"
  ],
  "repository": "git@github.com:sohale/jrep.git",
  "author": "Sohail Siadatnejad <sohale@gmail.com>",
  "license": "GPL",
  "private": false,
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "9.2.2",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "check-coverage": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 90,
    "exclude": [
      "test/**",
      ".eslintrc.js",
      "codebin",
      "tests",
      "coverage"
    ]
  }
}
